    "@typescript-eslint/parser": "^5.59.8",
    "concurrently": "^8.2.1",
    "eslint": "^8.42.0",
    "eslint-plugin-svelte": "^2.30.0",
    "svelte": "^3.59.1",
    "svelte-check": "^2.10.3",
    "tslib": "^2.5.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.2",
    "vitest": "^0.31.4"
  },
  "dependencies": {
